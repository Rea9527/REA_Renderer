{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/builds/a.exe",
            "preLaunchTask": "Build",
            "cwd": "${workspaceRoot}",
            "environment": [],
            "stopAtEntry": true,
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "C:/MinGW/bin/gdb.exe"
        }
    ]
}